//0344. Reverse String

class Solution 
{
    public:
        void reverseString(vector<char>& String) 
        {
            int Length = String.size();

            char Temp;

            for(int X = 0; X < (Length/2); X++)
            {
                Temp = String[X];

                String[X] = String[Length - X - 1];
                String[Length - X - 1] = Temp;
            }
        }
};
