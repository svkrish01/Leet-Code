//1304. Find N Unique Integers Sum up to Zero

class Solution 
{
    public:
        vector<int> sumZero(int N) 
        {
            vector<int> Numbers;

            for(int X = 1; X < N; X++)
            {
                Numbers.push_back(X);
            }

            Numbers.push_back(-(((N) * (N-1)) / 2));

            return Numbers;
        }
};
