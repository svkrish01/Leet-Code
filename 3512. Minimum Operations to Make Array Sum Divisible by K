//3512. Minimum Operations to Make Array Sum Divisible by K

class Solution 
{
    public:
        int minOperations(vector<int>& Nums, int K) 
        {
            int Sum = 0;

            for(int Temp : Nums)
            {
                Sum += Temp;
            }

            return Sum % K;
        }
};
