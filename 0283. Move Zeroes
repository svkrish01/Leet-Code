//0283. Move Zeroes

class Solution 
{
    public:
        void moveZeroes(vector<int>& Numbers) 
        {
            int Length = Numbers.size();

            for(int X = 0; X < Length; X++)
            {
                if(Numbers[X] == 0)
                {
                    Numbers.erase(Numbers.begin() + X);
                    Numbers.push_back(0);

                    Length--;
                    X--;
                }
            }
        }
};
